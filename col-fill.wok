#!/usr/bin/wok -f

/** col-fill.wok
  * 
  * Usage:
  *   java -jar wok-0.1.0.jar -f col-fill.wok -v idx=0 input > output
  *
  * Option:
  *   -v idx:        The index number of the column to be filled. This value must be larger than or equal to zero.
  */

FS = '\t'
FQ = Quote.Min
OFS = '\t'
OFQ = Quote.All

In { _
  .map (_.padTo(idx+1, ""))
  .foreach (row => println(row: _*))
}
